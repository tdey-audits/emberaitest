# Server configuration
PORT=3000

# A2A configuration
# Note: Agent URL is configured in config/agent.md (card.url field)
A2A_SERVER_LOGGING=false

# AI provider configuration
AI_PROVIDER=openrouter

# AI provider API keys (configure the provider you're using)
OPENROUTER_API_KEY=
OPENAI_API_KEY=
XAI_API_KEY=
HYPERBOLIC_API_KEY=

# Tool configuration
AGENT_TOOL_TIMEOUT_MS=60000

# Wallet configuration (optional)
TEST_PRIVATE_KEY=0x
RPC_URL=

# Data feeds configuration
# Chainlink Data Streams
CHAINLINK_WS_URL=wss://streams.chainlink.example.com
CHAINLINK_REST_URL=https://api.chainlink.example.com
CHAINLINK_API_KEY=

# GMX Oracle Feeds
GMX_WS_URL=wss://api.gmx.io/ws
GMX_REST_URL=https://api.gmx.io
GMX_CHAIN_ID=42161

# Data feeds options
FEEDS_USE_WEBSOCKET=true
FEEDS_CACHE_TTL_MS=60000
FEEDS_RECONNECT_INTERVAL_MS=5000
FEEDS_MAX_RECONNECT_ATTEMPTS=10
FEEDS_HEARTBEAT_INTERVAL_MS=30000

# Logging configuration
LOG_LEVEL=info
LOG_STRUCTURED=true
