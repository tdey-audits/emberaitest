{
  "name": "agent-node",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@a2a-js/sdk": "^0.3.3",
    "@ai-sdk/openai": "^2.0.42",
    "@ai-sdk/provider-utils": "^3.0.8",
    "@ai-sdk/xai": "^2.0.23",
    "@hyperbolic/ai-sdk-provider": "^0.1.3",
    "@modelcontextprotocol/sdk": "catalog:",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "ai": "catalog:",
    "ajv": "^8.17.1",
    "cors": "^2.8.5",
    "express": "catalog:",
    "gray-matter": "^4.0.3",
    "jiti": "^2.6.1",
    "json-schema-to-zod": "^2.6.1",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1",
    "uuid": "^13.0.0",
    "viem": "^2.37.2",
    "ws": "^8.18.3",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/eventsource": "^3.0.0",
    "@types/express": "^5.0.3",
    "@types/json-schema": "^7.0.15",
    "@types/node": "catalog:",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axios": "^1.12.2",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eventsource": "^4.0.0",
    "fast-check": "^4.3.0",
    "msw": "^2.11.2",
    "prettier": "catalog:",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "scripts": {
    "dev": "tsx watch src/server-loader.ts",
    "build": "rimraf dist .tsbuildinfo && tsc -p tsconfig.build.json",
    "start": "node dist/server-loader.js",
    "cli": "tsx src/cli/loader.ts",
    "client": "tsx src/a2aClient.ts",
    "clean": "rimraf node_modules dist .tsbuildinfo",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint . --fix && prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:int",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:int": "tsx --env-file=.env.test ./node_modules/vitest/vitest.mjs run --config vitest.config.int.ts",
    "test:e2e": "tsx --env-file=.env.test ./node_modules/vitest/vitest.mjs run --config vitest.config.e2e.ts",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:record-mocks": "tsx --env-file=.env tests/utils/record-mocks.ts",
    "test:ci": "pnpm test:unit && pnpm test:int",
    "test:ci:main": "pnpm test:unit && pnpm test:int && pnpm test:e2e",
    "precommit": "pnpm lint && pnpm typecheck && pnpm test",
    "ci": "pnpm install --frozen-lockfile && pnpm lint && pnpm typecheck && pnpm test:ci && pnpm build",
    "ci:main": "pnpm install --frozen-lockfile && pnpm lint && pnpm typecheck && pnpm test:ci:main && pnpm build"
  }
}
