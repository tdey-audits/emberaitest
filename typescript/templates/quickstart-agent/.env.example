# AI Provider Configuration
# Uncomment and set the API key for your preferred provider

# OpenRouter (supports multiple models)
# OPENROUTER_API_KEY=your-openrouter-api-key

# OpenAI
# OPENAI_API_KEY=your-openai-api-key

# XAI (Grok)
# XAI_API_KEY=your-xai-api-key

# Hyperbolic
# HYPERBOLIC_API_KEY=your-hyperbolic-api-key

# Provider Selection (optional)
# If not set, the first available provider will be used
# AI_PROVIDER=openrouter|openai|xai|hyperbolic

# Model Selection (optional)
# If not set, the default model for the selected provider will be used
# AI_MODEL=your-preferred-model

# Agent Configuration
PORT=3007
NODE_ENV=development

# Agent Configuration (optional)
AGENT_NAME=Hello Quickstart Agent
AGENT_VERSION=1.0.0
AGENT_DESCRIPTION=A comprehensive example demonstrating all v2 framework features

# Logging Configuration (optional)
LOG_LEVEL=debug

# CORS Configuration (optional)
# Set to false to disable CORS
ENABLE_CORS=true

# Base Path Configuration (optional)
# Example: /api/v1
BASE_PATH=
