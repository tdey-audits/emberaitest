# Ember Agent Configuration

# AI Provider API Keys (at least one required)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
XAI_API_KEY=your_xai_api_key_here
HYPERBOLIC_API_KEY=your_hyperbolic_api_key_here

# AI Configuration
# Available providers: openrouter, openai, xai, hyperbolic
AI_PROVIDER=openrouter
# Model override (optional, defaults to provider's default model)
# AI_MODEL=google/gemini-2.0-flash

# Agent Configuration (optional)
# AGENT_NAME=Ember Agent
# AGENT_VERSION=1.0.0
# AGENT_DESCRIPTION=Unified multi-skill DeFi agent supporting swapping, lending, liquidity, and yield trading operations on Arbitrum
# AGENT_URL=localhost

# Server Configuration
PORT=3001
ENABLE_CORS=true
# BASE_PATH=/

# Blockchain Configuration (required)
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# Ember MCP Server Configuration (required)
EMBER_MCP_SERVER_URL=https://api.emberai.xyz/mcp

# Optional Configuration
# MCP_TOOL_TIMEOUT_MS=120000
# DEFAULT_USER_ADDRESS=0x...
# AGENT_CACHE_TOKENS=false
# NODE_ENV=development
